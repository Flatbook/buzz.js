{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./src/index.ts"],
  "excludeExternals": true,
  "out": "./docs/generated",
  "readme": "none",
  "excludeInternal": true,
  "plugin": ["typedoc-plugin-markdown"],
  "exclude": ["node_modules", "dist"],
  "name": "buzz.js Documentation",
  "categoryOrder": [
    "Core Functions",
    "Apollo Client Mocks",
    "Validators",
    "Schema Management",
    "Utilities"
  ],
  "categorizeByGroup": true,
  "defaultCategory": "Core Functions",
  "sort": ["source-order"],
  "includeVersion": true,
  "hideGenerator": true,
  "theme": "default"
}
